(snip|snippet) talon file:
    user.vim_insert_mode("tf")
    key(tab)

(snip|snippet) insert [action]:
    user.vim_insert_mode("tsi")
    key(tab)

(snip|snippet) big insert [action]:
    user.vim_insert_mode("tmi")
    key(tab)

(snip|snippet) key [action]:
    user.vim_insert_mode("tk")
    key(tab)

(snip|snippet) (snip|snippet) [action]:
    user.vim_insert_mode("tsnip")
    key(tab)
